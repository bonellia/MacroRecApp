        <attr format="integer" name="points"/>
        <attr format="integer" name="max"/>
        <attr format="integer" name="min"/>
        <attr format="integer" name="step"/>
        <attr format="reference" name="indicatorIcon"/>
        <attr format="dimension" name="progressWidth"/>
        <attr format="color" name="progressColor"/>
        <attr format="dimension" name="arcWidth"/>
        <attr format="color" name="arcColor"/>
        <attr format="dimension" name="textSize"/>
        <attr format="color" name="textColor"/>
